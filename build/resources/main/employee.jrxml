    <parameter name="lastName" class="java.lang.String"> </parameter>
    <parameter name="testingParameter" class="java.lang.String"/>

    <title>
        <band height="20" splitType="Stretch">
            <textField>
                <reportElement x="238" y="0" width="100" height="20"/>
                <textElement/>
                <textFieldExpression class="java.lang.String">
<!--                    <![CDATA[$P{title}]]>-->
<!--                    [CDATA[$P{testingParameter}]]-->
                    <![CDATA[$P{testingParameter}]]>
                </textFieldExpression>
            </textField>
            <textField>
                <reportElement x="0" y="0" width="100" height="20"/>
                <textElement/>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[$P{testingParameter}]]>
                </textFieldExpression>
            </textField>
        </band>
    </title>

    <detail>
        <band height="20" splitType="Stretch">
            <textField>
                <reportElement x="20" y="0" width="100" height="20"/>
                <textElement/>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[$P{title}]]>
                </textFieldExpression>
            </textField>

        </band>

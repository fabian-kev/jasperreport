<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
              name="personal-details"


              leftMargin="0"
              rightMargin="0">
    <template>"styles.jrtx"</template>


    <parameter name="employee" class="com.fabiankevin.jasper.Employee"></parameter>

    <title>
        <band height="100" splitType="Stretch">
            <textField>
                <reportElement x="0" y="0" width="500" height="20" style="styled-tf"/>
                <textElement markup="styled">
                </textElement>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[("First Name: <style pdfFontName = 'Helvetica'>Kevin</style>")]]>
                </textFieldExpression>
            </textField>

            <textField>
                <reportElement x="250" y="0" width="500" height="20" style="styled-tf"/>
                <textElement markup="styled">
                </textElement>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[("Middle Name: <style pdfFontName = 'Helvetica'>Lopez</style>")]]>
                </textFieldExpression>

            </textField>

            <textField>
                <reportElement x="450" y="0" width="500" height="20" style="styled-tf"/>
                <textElement markup="styled">
                </textElement>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[("Last Name: <style pdfFontName = 'Helvetica'>De Jesus</style>")]]>
                </textFieldExpression>
            </textField>


            <!--            GENDER-->
            <textField>
                <reportElement x="0" y="20" width="500" height="20" style="styled-tf"/>
                <textElement markup="styled">
                </textElement>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[("Gender: <style pdfFontName = 'Helvetica'>Male</style>")]]>
                </textFieldExpression>
            </textField>

            <textField>
                <reportElement x="250" y="20" width="500" height="20" style="styled-tf"/>
                <textElement markup="styled">
                    <font isBold="true"/>
                </textElement>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[("Nationality: <style pdfFontName = 'Helvetica'>Filipino</style>")]]>
                </textFieldExpression>
            </textField>

            <textField>
                <reportElement x="450" y="20" width="500" height="20" style="styled-tf"/>
                <textElement markup="styled">
                    <!--                    <font size = "22"/>-->
                </textElement>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[("Date of Birth: <style pdfFontName = 'Helvetica'>January 15, 1980</style>")]]>
                </textFieldExpression>
            </textField>

            <!--            &lt;!&ndash;            PLACE OF BIRTH&ndash;&gt;\-->
            <textField>
                <reportElement x="0" y="40" width="500" height="20" style="styled-tf"/>
                <textElement markup="styled">
                </textElement>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[( "Place of Birth: <style pdfFontName = 'Helvetica'>Makati City</style>")]]>
                </textFieldExpression>
            </textField>


            <textField>
                <reportElement x="250" y="40" width="500" height="20" style="styled-tf"/>
                <textElement markup="styled">
                </textElement>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[( "Civil Status: <style pdfFontName = 'Helvetica'>Married</style>")]]>
                </textFieldExpression>
            </textField>

            <textField>
                <reportElement x="450" y="40" width="500" height="20" style="styled-tf"/>
                <textElement markup="styled">
                </textElement>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[( "Name of Spouse: <style pdfFontName = 'Helvetica'>Maria Mercedes Soriano</style>")]]>
                </textFieldExpression>
            </textField>


            <!--            mother's name-->


            <textField>
                <reportElement x="0" y="60" width="500" height="20" style="styled-tf"/>
                <textElement markup="styled">
                    <!--                    <font size = "22"/>-->
                </textElement>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[("Mother's Maiden Name: <style pdfFontName = 'Helvetica'>Juana Santos Martinez</style>")]]>
                </textFieldExpression>
            </textField>

            <textField>
                <reportElement x="250" y="60" width="500" height="20" style="styled-tf"/>
                <textElement markup="styled">
                    <!--                    <font size = "22"/>-->
                </textElement>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[("SSS No. / GSIS No.: <style pdfFontName = 'Helvetica'>123456789</style>")]]>
                </textFieldExpression>
            </textField>


            <textField>
                <reportElement x="450" y="60" width="500" height="20" style="styled-tf"/>
                <textElement markup="styled">
                    <!--                    <font size = "22"/>-->
                </textElement>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[("TIN: <style pdfFontName = 'Helvetica'>123456789</style>")]]>
                </textFieldExpression>
            </textField>
            <line>
                <reportElement x="-5" y="80" width="635" height="0"/>
            </line>

        </band>
    </title>
</jasperReport>
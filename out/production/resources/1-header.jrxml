<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
              name="header-details"
              topMargin="0"
              leftMargin="0"
              rightMargin="0">
    <template>"styles.jrtx"</template>



    <title>
        <band height="100" splitType="Stretch">
            <textField>
                <reportElement x="0" y="0" width="500" height="20" style="styled-tf"/>
                <textElement markup="styled">
                </textElement>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[("Green - <style pdfFontName = 'Helvetica-Oblique'>Items auto populated from Customer encoding DOB app;</style>")]]>
                </textFieldExpression>
            </textField>

            <textField>
                <reportElement x="300" y="0" width="300" height="20" forecolor="#AB47BC" style="styled-tf"/>
                <textElement markup="styled">
                </textElement>
                <textFieldExpression class="java.lang.String">
                    <![CDATA[("Purple - <style pdfFontName = 'Helvetica-Oblique'>To be accomplished by Customer when he visits Branch</style>")]]>
                </textFieldExpression>
            </textField>
        </band>
    </title>
</jasperReport>